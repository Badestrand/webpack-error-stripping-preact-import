{
	"presets": [
		"@babel/preset-env",
		"@babel/preset-react",
		"@babel/preset-typescript"
	],
	"plugins": [
		[
			"@babel/plugin-transform-runtime", {
				"regenerator": true
			}
		],
		["@babel/plugin-transform-react-jsx", {
			"pragma": "preact.h",
			"pragmaFrag": "preact.Fragment",
		}]
	]
}
